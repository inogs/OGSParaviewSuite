                        number_of_elements="1"
                        default_values="0">
        <BooleanDomain name="bool"/>
        <Documentation>
          If this property is set to 1, the density will be used
          to compute the MLD.
        </Documentation>
      </IntVectorProperty>
-->
      <!-- drho 
      <DoubleVectorProperty name="drho"
                            label="Temperature difference: "
                            command="Setdrho"
                            animateable="1"
                            default_values="0.03"
                            number_of_elements="1">
        <Documentation>
          Density difference.
        </Documentation>
      </DoubleVectorProperty>
-->
      <!-- Zref 
      <DoubleVectorProperty name="zref"
                            label="Reference depth: "
                            command="Setzref"
                            animateable="1"
                            default_values="10."
                            number_of_elements="1"
                            panel_visibility="advanced">
        <Documentation>
          Reference depth.
        </Documentation>
      </DoubleVectorProperty>

      <PropertyGroup label="MLD parameters">
        <Property name="dT"/>
        <Property name="UseDensity"/>
        <Property name="drho"/>
        <Property name="zref"/>
